<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".hygiene_vie">

    <Button
        android:id="@+id/buttonfed"
        android:layout_width="329dp"
        android:layout_height="55dp"
        android:layout_marginTop="4dp"
        android:onClick="on_Click"
        android:text="@string/federation_cardiology"
        app:iconTint="#000000"
        app:layout_constraintEnd_toEndOf="@+id/nextstep5"
        app:layout_constraintStart_toStartOf="@+id/previousstep4"
        app:layout_constraintTop_toBottomOf="@+id/nextstep5" />

    <Button
        android:id="@+id/previousstep4"
        android:layout_width="160dp"
        android:layout_height="54dp"
        android:layout_marginStart="268dp"
        android:onClick="action_previous_page"
        android:text="@string/Previous_Step"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/nextstep5"
        app:layout_constraintHorizontal_bias="0.862"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.028" />

    <Button
        android:id="@+id/nextstep5"
        android:layout_width="160dp"
        android:layout_height="54dp"
        android:layout_marginEnd="92dp"
        android:onClick="action_next_page"
        android:text="@string/next_step"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.592"
        app:layout_constraintStart_toEndOf="@+id/previousstep2"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.028" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="175dp"
        android:layout_height="93dp"
        android:layout_marginTop="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.023"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/hygiene_vie" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="384dp"
        android:layout_height="44dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="124dp"
        android:text="@string/drink"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <RadioButton
        android:id="@+id/noAlcohol"
        android:layout_width="100dp"
        android:layout_height="50dp"
        android:layout_marginStart="4dp"
        android:text="No"
        app:layout_constraintBottom_toBottomOf="@+id/yesAlcohol"
        app:layout_constraintStart_toEndOf="@+id/yesAlcohol"
        app:layout_constraintTop_toBottomOf="@+id/textView11"
        app:layout_constraintVertical_bias="0.0" />

    <RadioButton
        android:id="@+id/yesAlcohol"
        android:layout_width="100dp"
        android:layout_height="50dp"
        android:layout_marginStart="96dp"
        android:text="Yes"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView11" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="96dp"
        android:layout_marginTop="12dp"
        android:text="@string/energy"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/noAlcohol" />

    <RadioButton
        android:id="@+id/noEnergyDrink"
        android:layout_width="100dp"
        android:layout_height="50dp"
        android:layout_marginStart="4dp"
        android:text="No"
        app:layout_constraintBottom_toBottomOf="@+id/yesEnergyDrink"
        app:layout_constraintStart_toEndOf="@+id/yesEnergyDrink"
        app:layout_constraintTop_toBottomOf="@+id/textView13"
        app:layout_constraintVertical_bias="1.0" />

    <RadioButton
        android:id="@+id/yesEnergyDrink"
        android:layout_width="100dp"
        android:layout_height="50dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="4dp"
        android:text="Yes"
        app:layout_constraintStart_toStartOf="@+id/textView13"
        app:layout_constraintTop_toBottomOf="@+id/textView13" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:text="@string/sleep"
        app:layout_constraintStart_toStartOf="@+id/yesSleepDisorders"
        app:layout_constraintTop_toBottomOf="@+id/noSleepDisorders" />

    <RadioButton
        android:id="@+id/noSleepHours"
        android:layout_width="100dp"
        android:layout_height="50dp"
        android:text="No"
        app:layout_constraintBottom_toBottomOf="@+id/yesSleepHours"
        app:layout_constraintStart_toEndOf="@+id/yesSleepHours"
        app:layout_constraintTop_toBottomOf="@+id/textView14"
        app:layout_constraintVertical_bias="1.0" />

    <RadioButton
        android:id="@+id/yesSleepHours"
        android:layout_width="100dp"
        android:layout_height="50dp"
        android:layout_marginTop="4dp"
        android:layout_marginEnd="108dp"
        android:text="Yes"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/textView14"
        app:layout_constraintTop_toBottomOf="@+id/textView14" />

    <TextView
        android:id="@+id/textView15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="124dp"
        android:layout_marginEnd="100dp"
        android:text="@string/sleep_disorders"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <RadioButton
        android:id="@+id/yesSleepDisorders"
        android:layout_width="101dp"
        android:layout_height="50dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="124dp"
        android:text="Yes"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/textView15"
        app:layout_constraintTop_toBottomOf="@+id/textView15" />

    <RadioButton
        android:id="@+id/noSleepDisorders"
        android:layout_width="100dp"
        android:layout_height="50dp"
        android:text="No"
        app:layout_constraintBottom_toBottomOf="@+id/yesSleepDisorders"
        app:layout_constraintStart_toEndOf="@+id/yesSleepDisorders"
        app:layout_constraintTop_toBottomOf="@+id/textView15"
        app:layout_constraintVertical_bias="1.0" />

</androidx.constraintlayout.widget.ConstraintLayout>