<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".hygiene_vie">

    <Button
        android:id="@+id/nextstep5"
        android:layout_width="160dp"
        android:layout_height="54dp"
        android:layout_marginEnd="36dp"
        android:layout_marginBottom="132dp"
        android:onClick="action_next_page"
        android:text="@string/next_step"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/previousstep4"
        app:layout_constraintTop_toBottomOf="@+id/cholesterol"
        app:layout_constraintVertical_bias="1.0" />

    <RadioButton
        android:id="@+id/yesAlcohol"
        android:layout_width="175dp"
        android:layout_height="58dp"
        android:text="Yes"
        app:layout_constraintEnd_toStartOf="@+id/noAlcohol"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/textView11"
        app:layout_constraintTop_toBottomOf="@+id/textView11" />

    <RadioButton
        android:id="@+id/noAlcohol"
        android:layout_width="200dp"
        android:layout_height="58dp"
        android:layout_marginEnd="20dp"
        android:text="No"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView11" />

    <RadioButton
        android:id="@+id/noSleepHours"
        android:layout_width="198dp"
        android:layout_height="56dp"
        android:text="No"
        app:layout_constraintBottom_toTopOf="@+id/nextstep5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/yesSleepHours"
        app:layout_constraintTop_toTopOf="@+id/yesSleepHours"
        app:layout_constraintVertical_bias="0.0" />

    <RadioButton
        android:id="@+id/yesSleepHours"
        android:layout_width="176dp"
        android:layout_height="59dp"
        android:text="Yes"
        app:layout_constraintBottom_toTopOf="@+id/previousstep4"
        app:layout_constraintStart_toStartOf="@+id/textView14"
        app:layout_constraintTop_toBottomOf="@+id/textView14"
        app:layout_constraintVertical_bias="0.0" />

    <RadioButton
        android:id="@+id/yesSleepDisorders"
        android:layout_width="177dp"
        android:layout_height="61dp"
        android:text="Yes"
        app:layout_constraintStart_toStartOf="@+id/textView15"
        app:layout_constraintTop_toBottomOf="@+id/textView15" />

    <RadioButton
        android:id="@+id/noSleepDisorders"
        android:layout_width="198dp"
        android:layout_height="62dp"
        android:text="No"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/yesSleepDisorders"
        app:layout_constraintTop_toTopOf="@+id/yesSleepDisorders" />

    <RadioButton
        android:id="@+id/noEnergyDrink"
        android:layout_width="201dp"
        android:layout_height="61dp"
        android:layout_marginEnd="20dp"
        android:text="No"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView13" />

    <RadioButton
        android:id="@+id/yesEnergyDrink"
        android:layout_width="177dp"
        android:layout_height="61dp"
        android:text="Yes"
        app:layout_constraintEnd_toStartOf="@+id/noEnergyDrink"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/textView13"
        app:layout_constraintTop_toBottomOf="@+id/textView13" />

    <Button
        android:id="@+id/previousstep4"
        android:layout_width="160dp"
        android:layout_height="54dp"
        android:layout_marginStart="32dp"
        android:layout_marginBottom="132dp"
        android:onClick="action_previous_page"
        android:text="@string/Previous_Step"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/nextstep"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="175dp"
        android:layout_height="93dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.025"
        app:srcCompat="@drawable/hygiene_vie" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="384dp"
        android:layout_height="44dp"
        android:layout_marginTop="24dp"
        android:text="@string/drink"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.407"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView7" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="@string/energy"
        app:layout_constraintStart_toStartOf="@+id/textView11"
        app:layout_constraintTop_toBottomOf="@+id/yesAlcohol" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="@string/sleep"
        app:layout_constraintStart_toStartOf="@+id/textView15"
        app:layout_constraintTop_toBottomOf="@+id/yesSleepDisorders" />

    <TextView
        android:id="@+id/textView15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="@string/sleep_disorders"
        app:layout_constraintStart_toStartOf="@+id/textView13"
        app:layout_constraintTop_toBottomOf="@+id/yesEnergyDrink" />

    <Button
        android:id="@+id/buttonfed"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="60dp"
        android:onClick="on_Click"
        android:text="@string/federation_cardiology"
        app:iconTint="#000000"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.494"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/previousstep4" />

</androidx.constraintlayout.widget.ConstraintLayout>